#!/bin/bash

echo "$@" >> /home/mythmon/.launcher.log

path=$1
echo $path

if [[ "$path" == 'subl://'* ]]; then
    path=$(echo $path | cut -c 8-)
    exec subl $path
elif [[ "$path" =~ https?://www.youtube.com/watch\?v=.* ]]; then
    exec yt "$path"
elif [[ "$path" == 'file://'* ]]; then
    path=$(echo $path | cut -c 8-)
    exec subl $path
else
    exec browser $path
fi
